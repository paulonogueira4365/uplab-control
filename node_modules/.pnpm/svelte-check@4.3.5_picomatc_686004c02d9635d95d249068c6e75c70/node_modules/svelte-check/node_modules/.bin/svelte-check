#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules/svelte-check/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules/svelte-check/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/svelte-check@4.3.5_picomatc_686004c02d9635d95d249068c6e75c70/node_modules:/mnt/c/Users/User/Desktop/UPLAB USER/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
